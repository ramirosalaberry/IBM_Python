    "import pandas as pd\n",
    "\n",
    "reader=pd.read_stata(\"data_belen.dta\",chunksize=100000)\n",
    "\n",
    "df = pd.DataFrame()\n",
    "\n",
    "for itm in reader:\n",
    "    df=df.append(itm)\n",
    "df\n",
    "\n",
    "df.to_csv(\"data_belen.csv\")"
